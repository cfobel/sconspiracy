<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<%
import os
if not TYPE == 'exec':
    if os.path.exists(PROFILE_DIR) and TYPE == 'bundle':
        profile = os.path.join(PROFILE_DIR, 'profile.xml')
        if os.path.exists(profile):
            ARGUMENT = profile 
            EXECUTABLE = LAUNCHER_PATH
        else:
            ARGUMENT = ''
            EXECUTABLE = ''

    else:
        ARGUMENT = ''
        EXECUTABLE = ''
else:
    ARG = ''
    EXECUTABLE = EXEC_PATH

   
%>
<launchConfiguration type="org.eclipse.cdt.launch.applicationLaunchType">
<intAttribute key="org.eclipse.cdt.launch.ATTR_BUILD_BEFORE_LAUNCH_ATTR" value="2"/>
<stringAttribute key="org.eclipse.cdt.launch.COREFILE_PATH" value="${ARGUMENT}"/>
<stringAttribute key="org.eclipse.cdt.launch.PROGRAM_ARGUMENTS" value="test"/>
<stringAttribute key="org.eclipse.cdt.launch.PROGRAM_NAME" value="${EXECUTABLE}"/>
<stringAttribute key="org.eclipse.cdt.launch.PROJECT_ATTR" value="${ROOT_PROJECT}"/>
<stringAttribute key="org.eclipse.cdt.launch.PROJECT_BUILD_CONFIG_ID_ATTR" value="0.9263828609.8499039860"/>
<booleanAttribute key="org.eclipse.cdt.launch.use_terminal" value="true"/>
<listAttribute key="org.eclipse.debug.core.MAPPED_RESOURCE_PATHS">
<listEntry value="/${ROOT_PROJECT}"/>
</listAttribute>
<listAttribute key="org.eclipse.debug.core.MAPPED_RESOURCE_TYPES">
<listEntry value="4"/>
</listAttribute>
</launchConfiguration>
