# vim:filetype=python

@register
class FITS(LibExt):
    register_names = ['fits']

    version = '999'

    libs           = []

    debug_suffix   = "_D"

    def darwin(self):
        self.libs = ['cfitsio', 'fits2itk_libwcs', 'fits2itk_da_util']
        cpppath = [
                ('include','cfitsio'),
                ('include','fits2itk_libwcs'),
                ('include','fits2itk_da_util')
                ]
